{
  "WeatherSettings": {
    "ApiKey": "<API KEY>",
    "BaseUrl": "https://api.openweathermap.org/data/2.5/"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "System.Net.Http.HttpClient": "Information"
      },
      "Using": [ "Serilog.Sinks.Console" ]
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  }
}
